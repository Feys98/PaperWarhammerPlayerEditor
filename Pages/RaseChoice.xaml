<Page x:Class="WHeditor.RaseChoice"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WHeditor"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RaseChoice" Width="1024" Height="720">

    <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Height="720" >

        <Image x:Name="RaceChoiceImgHumanIcon" 
               Source="/WHeditor;component/Img/HumanIcon.png" 
               MouseLeftButtonDown="RaceChoiceImgWariorIcon_MouseLeftButtonDown" VerticalAlignment="Top" MouseEnter="MouseTestFunc" HorizontalAlignment="Center" Width="100" Height="100" Margin="99,54,825,0" />







        <Image x:Name="RaceChoiceImgElfIcon" 
            Source="/WHeditor;component/Img/ElfIcon.png" 
             Height="120" Margin="99,210,825,0" VerticalAlignment="Top" />
        <Image x:Name="RaceChoiceImgDwarfIcon" 
            Source="/WHeditor;component/Img/DwarfIcon.png" 
            Height="110" Margin="99,374,825,0" VerticalAlignment="Top" />
        <Image x:Name="RaceChoiceImgHalflingIcon" 
            Source="/WHeditor;component/Img/HalflingIcon.png" 
            Height="100" Margin="99,542,825,0" VerticalAlignment="Top" />

    </Grid>
</Page>
